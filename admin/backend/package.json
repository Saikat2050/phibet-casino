{
  "name": "admin-backend",
  "version": "1.0.0",
  "description": "Backend for admin related APIs",
  "main": "index.js",
  "engines": {
    "node": "v22.14.0",
    "npm": "10.9.2"
  },
  "scripts": {
    "babel-node": "babel-node",
    "build": "babel . -d dist --copy-files --delete-dir-on-start",
    "ob-build": "javascript-obfuscator --compact true --self-defending true dist --output dist",
    "start": "node ./dist/index",
    "start:dev": "nodemon --exec npm run babel-node -- ./index.js",
    "lint": "standard --fix",
    "db:migrate": "node ./node_modules/.bin/sequelize-cli db:migrate --config dist/src/configs/database.config.js",
    "db:seed": "node ./node_modules/.bin/sequelize-cli db:seed:all --config dist/src/configs/database.config.js",
    "db:init": "node ./node_modules/.bin/sequelize-cli db:create --config dist/src/configs/database.config.js",
    "db:reset": "node ./node_modules/.bin/sequelize-cli db:drop --config dist/src/configs/database.config.js && node ./node_modules/.bin/sequelize-cli db:create --config dist/src/configs/database.config.js && node ./node_modules/.bin/sequelize-cli db:migrate --config dist/src/configs/database.config.js && node ./node_modules/.bin/sequelize-cli db:seed:all --config dist/src/configs/database.config.js",
    "migrate": "npm run babel-node node_modules/.bin/sequelize-cli db:migrate",
    "seed": "npm run babel-node node_modules/.bin/sequelize-cli db:seed:all",
    "init": "npm run babel-node node_modules/.bin/sequelize-cli db:create && npm run babel-node node_modules/.bin/sequelize-cli db:migrate && npm run babel-node node_modules/.bin/sequelize-cli db:seed:all",
    "reset": "npm run babel-node node_modules/.bin/sequelize-cli db:drop && npm run babel-node node_modules/.bin/sequelize-cli db:create && npm run babel-node node_modules/.bin/sequelize-cli db:migrate && npm run babel-node node_modules/.bin/sequelize-cli db:seed:all"
  },
  "author": "Phibet",
  "license": "ISC",
  "dependencies": {
    "@alt3/sequelize-to-json-schemas": "0.3.56",
    "@aws-sdk/client-s3": "3.511.0",
    "@aws-sdk/lib-storage": "3.511.0",
    "@faker-js/faker": "8.4.1",
    "@socket.io/redis-adapter": "8.2.1",
    "@socket.io/redis-emitter": "5.1.0",
    "ajv": "8.8.2",
    "ajv-formats": "2.1.1",
    "ajv-keywords": "5.1.0",
    "axios": "1.6.7",
    "bcrypt": "5.0.1",
    "child_process": "1.0.2",
    "convict": "6.2.4",
    "cors": "2.8.5",
    "crypto-js": "^4.2.0",
    "dayjs": "1.11.10",
    "dotenv": "10.0.0",
    "express": "4.18.2",
    "express-basic-auth": "1.2.1",
    "flatted": "3.2.4",
    "helmet": "6.0.1",
    "http-status-codes": "2.2.0",
    "ioredis": "5.3.1",
    "javascript-obfuscator": "4.1.1",
    "json2csv": "5.0.7",
    "jsonwebtoken": "9.0.2",
    "lodash": "4.17.21",
    "morgan": "1.10.0",
    "multer": "1.4.5-lts.1",
    "node-cache": "5.1.2",
    "node-mailjet": "6.0.5",
    "number-precision": "1.6.0",
    "pg": "8.6.0",
    "pg-hstore": "2.3.4",
    "prettier": "3.2.5",
    "sequelize": "6.34.0",
    "sequelize-cli": "6.6.1",
    "socket.io": "4.7.4",
    "umzug": "3.8.0",
    "uuid": "9.0.1",
    "web-push": "3.6.7",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "latest",
    "@babel/core": "latest",
    "@babel/eslint-parser": "latest",
    "@babel/node": "latest",
    "@babel/preset-env": "latest",
    "babel-plugin-module-resolver": "latest",
    "eslint": "7.12.1",
    "husky": "7.0.4",
    "lint-staged": "12.1.2",
    "nodemon": "3.0.2",
    "standard": "16.0.4"
  },
  "standard": {
    "parser": "@babel/eslint-parser",
    "env": {
      "jest": true,
      "browser": true,
      "node": true
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint"
    ]
  }
}
