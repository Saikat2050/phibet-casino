{
  "name": "queue-worker",
  "version": "1.0.0",
  "description": "Casino aggregator",
  "main": "index.js",
  "engines": {
    "node": "v22.14.0",
    "npm": "10.9.2"
  },
  "scripts": {
    "prepare": "husky install",
    "lint": "standard --fix",
    "build": "babel . -d dist  --copy-files --delete-dir-on-start",
    "ob-build": "javascript-obfuscator --compact true --self-defending true dist --output dist",
    "start": "node ./dist/index",
    "start:workers": "node ./dist/startWorkers",
    "dev": "NODE_ENV=development babel-node ./bin",
    "babel-node": "babel-node --inspect=0.0.0.0:9229",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:dev:workers": "nodemon --exec npm run babel-node -- ./startWorkers.js",
    "start:dev": "nodemon --exec npm run babel-node -- ./index.js"
  },
  "author": "Phibet",
  "license": "ISC",
  "dependencies": {
    "@alt3/sequelize-to-json-schemas": "^0.3.56",
    "@bull-board/api": "3.11.0",
    "@bull-board/express": "3.11.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "@socket.io/redis-emitter": "5.1.0",
    "ajv": "8.8.2",
    "ajv-formats": "2.1.1",
    "ajv-i18n": "4.2.0",
    "ajv-keywords": "5.1.0",
    "axios": "1.6.7",
    "body-parser": "1.20.1",
    "bull": "4.8.3",
    "convict": "6.2.4",
    "dayjs": "^1.11.11",
    "dotenv": "10.0.0",
    "express": "4.18.2",
    "express-basic-auth": "^1.2.1",
    "flatted": "3.2.4",
    "helmet": "6.0.1",
    "http-status-codes": "2.2.0",
    "ioredis": "^5.3.2",
    "javascript-obfuscator": "4.1.1",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "9.0.2",
    "lodash": "4.17.21",
    "morgan": "1.10.0",
    "number-precision": "1.6.0",
    "pg": "8.6.0",
    "pg-hstore": "2.3.4",
    "sequelize": "6.34.0",
    "sha256": "^0.2.0",
    "socket.io": "4.7.4",
    "ssh2-sftp-client": "10.0.3",
    "uuid": "8.3.0",
    "winston": "3.3.3",
    "moment-timezone": "0.5.47"
  },
  "devDependencies": {
    "@babel/cli": "7.16.0",
    "@babel/core": "7.16.5",
    "@babel/eslint-parser": "7.16.5",
    "@babel/node": "7.16.5",
    "@babel/preset-env": "7.16.5",
    "babel-plugin-module-resolver": "5.0.0",
    "dotenv": "10.0.0",
    "eslint": "7.12.1",
    "husky": "7.0.4",
    "lint-staged": "12.1.2",
    "nodemon": "3.0.3",
    "standard": "16.0.4"
  },
  "standard": {
    "parser": "@babel/eslint-parser",
    "env": {
      "jest": true,
      "browser": true,
      "node": true
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint"
    ]
  }
}
