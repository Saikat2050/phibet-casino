{
  "name": "user-backend",
  "version": "1.0.0",
  "description": "Backend for user related APIs",
  "main": "index.js",
  "engines": {
    "node": "v22.14.0",
    "npm": "10.9.2"
  },
  "scripts": {
    "babel-node": "babel-node --inspect=0.0.0.0:9229",
    "build": "babel . -d dist --copy-files --delete-dir-on-start",
    "ob-build": "javascript-obfuscator --compact true --self-defending true dist --output dist",
    "start": "node ./dist/index",
    "start:dev": "nodemon --exec npm run babel-node -- ./index.js",
    "lint": "standard --fix"
  },
  "author": "Phibet",
  "license": "ISC",
  "dependencies": {
    "@alt3/sequelize-to-json-schemas": "0.3.56",
    "@aws-sdk/client-s3": "3.504.0",
    "@aws-sdk/client-ses": "^3.797.0",
    "@aws-sdk/client-sns": "^3.799.0",
    "@aws-sdk/lib-storage": "3.504.0",
    "@socket.io/redis-adapter": "8.0.0",
    "@socket.io/redis-emitter": "5.1.0",
    "ajv": "8.8.2",
    "ajv-formats": "2.1.1",
    "ajv-i18n": "4.2.0",
    "ajv-keywords": "5.1.0",
    "ajv-merge-patch": "5.0.1",
    "bcrypt": "5.0.1",
    "connect-redis": "7.1.0",
    "convict": "6.2.4",
    "cors": "2.8.5",
    "crypto-js": "^4.2.0",
    "currency-symbol-map": "5.1.0",
    "dayjs": "1.11.10",
    "dotenv": "10.0.0",
    "express": "4.18.2",
    "express-basic-auth": "1.2.1",
    "express-session": "1.17.3",
    "geoip-lite": "^1.4.10",
    "helmet": "6.0.1",
    "hi-base32": "^0.5.1",
    "http-status-codes": "2.2.0",
    "i18n": "0.15.1",
    "import": "0.0.6",
    "ioredis": "5.2.4",
    "javascript-obfuscator": "4.1.1",
    "jsonwebtoken": "9.0.2",
    "lodash": "4.17.21",
    "moment-timezone": "0.5.47",
    "morgan": "1.10.0",
    "multer": "1.4.5-lts.1",
    "node-cache": "5.1.2",
    "node-mailjet": "6.0.5",
    "number-precision": "1.6.0",
    "otpauth": "9.4.0",
    "pg": "8.6.0",
    "pg-hstore": "2.3.4",
    "randomstring": "^1.3.0",
    "request-ip": "3.3.0",
    "sequelize": "6.34.0",
    "sha256": "^0.2.0",
    "socket.io": "4.7.2",
    "socket.io-msgpack-parser": "3.0.2",
    "twilio": "^5.4.0",
    "unique-username-generator": "^1.3.0",
    "web-push": "^3.6.7",
    "web3": "^4.8.0",
    "winston": "3.3.3",
    "xml2js": "0.6.2"
  },
  "devDependencies": {
    "@babel/cli": "latest",
    "@babel/core": "latest",
    "@babel/eslint-parser": "latest",
    "@babel/node": "latest",
    "@babel/preset-env": "latest",
    "babel-plugin-module-resolver": "latest",
    "eslint": "7.12.1",
    "husky": "7.0.4",
    "lint-staged": "12.1.2",
    "nodemon": "3.0.2",
    "standard": "16.0.4"
  },
  "overrides": {
    "ajv-merge-patch": {
      "fast-json-patch": "3.1.1"
    }
  },
  "standard": {
    "parser": "@babel/eslint-parser",
    "env": {
      "jest": true,
      "browser": true,
      "node": true
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint"
    ]
  }
}
